{
  "name": "gitshipnote",
  "displayName": "GitShipNote",
  "description": "AI-powered changelog generator for Git repositories",
  "version": "0.0.1",
  "publisher": "BrianBaoHoang",
  "engines": {
    "vscode": "^1.85.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/baoblank25/Git_Ship_Note"
  },
  "categories": ["Other"],
  "activationEvents": ["onCommand:gitshipnote.generateChangelog"],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gitshipnote.generateChangelog",
        "title": "GitShipNote: Generate Changelog"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "axios": "^1.6.2"
  }
}
