{
  "name": "gitshipnote",
  "displayName": "GitShipNote",
  "description": "Generate beautiful changelogs from your git history using AI",
  "version": "1.0.1",
  "publisher": "BrianBaoHoang",
  "icon": "images/Icon.jpg",
  "engines": {
    "vscode": "^1.74.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/baoblank25/Git_Ship_Note"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gitshipnote.generateChangelog",
        "title": "GitShipNote: Generate Changelog"
      }
    ],
    "configuration": {
      "title": "GitShipNote",
      "properties": {
        "gitshipnote.anthropicApiKey": {
          "type": "string",
          "default": "",
          "description": "Your Anthropic API Key (get it from console.anthropic.com)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/node": "^16.x",
    "@types/vscode": "^1.74.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1"
  }
}
